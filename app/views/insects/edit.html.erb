<%= simple_form_for [ @insect ] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :order %>
  <%= f.text_field :order %>
  <%= f.label "ajouter des images" %>
  <%= f.file_field :photos, multiple: true %>
  <%= f.submit "Modifier l'insect", class: "btn btn-primary" %>
<% end %>
